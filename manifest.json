{
  "name": "AnuraOS",
  "description": "An extension that puts AnuraOS into a window.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://nomore.br3xality.org/*"
  ],
  "action": {
    "default_icon": {
     "128": "icons/icon128.png"
    },
    "default_title": "AnuraOS"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
